{
  "name": "bugtracker-backend-js",
  "version": "1.0.0",
  "description": "Backend written in JavaScript for a bug tracking system.",
  "type": "commonjs",
  "main": "server.js",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "build": "tsc",
    "run": "node dist/server.js",
    "build:watch": "tsc --watch",
    "run:watch": "nodemon dist/server.js --watch dist",
    "start": "npm-run-all --parallel build:watch run:watch"
  },
  "author": "Samuel Guebo",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "dotenv": "^16.0.3",
    "express": "^4.16.2",
    "first-run": "^3.0.0",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.19.2",
    "mysql2": "^2.3.3",
    "sequelize": "^6.25.3",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/source-map-support": "^0.5.6",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
